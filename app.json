
{
  "name": "Chat-Gpt",
  "description": "A Telegram open-ai Chatgpt bot.",
  "logo": "https://telegra.ph/file/5f1651ef2a992cef68f1e.jpg",
  "keywords": [
    "telegram", "chatgpt",
    "python",
    "pyrogram",
    "Abbu_yt7"   
  ],
  "stack": "heroku-20",
  "addons": [
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "12"
      }
    }
  ],
  "env": {
    "BOT_TOKEN": { "description": "telegram bot token here",
      "required": true
    },
    "OWNER_ID": {
      "description": "Owner id OF TELEGRAM ",
      "required": true
    },
    "SUPPORT_GRP": {
      "description": "Your Support Group without @",
      "required": true
    },
    "STKR": {
      "description": "Sticker for start", 
      "required": false
    },
    "START_IMG": {
      "description": "Start Image For Bot.",
      "required": true
    },
    "UPDATE_CHNL": {
      "description": "Your Updates Channel without @",
      "required": true
    },
    "API_ID": { "description": "telegram api id here",
      "required": true
    },
    "API_HASH": {
      "description": "telegram api hash here",
      "required": true
    }
  },
  "repository": "https://github.com/muiz12320/Chatgpt-bot1"
}
